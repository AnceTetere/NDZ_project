starpkodi3_11_29 <- function(y, t, prev, v) {
  
  if (all(diff(t$NDZ_sanemsanas_datums) == 0)) {
    if (t$zinkod[3] == "40") {
      yt <- y[v,]
      yt$dienas <- as.numeric(diff(t$NDZ_sanemsanas_datums[1:2]))
    } else {stop("Starpkodi3_11: Tr큰kst izstr훮des koda.")}
  } else {stop("Starpkodi3_11: Tr큰kst izstr훮des koda.")}

  rm(y, t, prev, v)
  return(yt)
}
