loadTable <- function(o) {
  #Ielādē tabulu

load(paste0("NDZ_", o, ".RData"))
x <- get(paste0("NDZ_", o))
rm(list = paste0("NDZ_", o))

#1 Sakārto tabulu
x <- x[order(x$ps_code, x$nr_code, x$NDZ_sanemsanas_datums), ]
#!!!! šitās rows izdzēs , jo te nekāda jēga no tām nav
#rows <- nrow(x) # priekš pārbaudes vai rindas sakrīt pirms un pēc apstrādes
rownames(x) <- NULL

x$start[is.na(x$start)] <- ""
x$dnperk[is.na(x$dnperk)] <- ""
x$end[is.na(x$end)] <- ""

return(x)
}
